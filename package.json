{
  "name": "@renovatebot/osv-offline-monorepo",
  "private": true,
  "type": "commonjs",
  "version": "0.0.0-semantic-release",
  "repository": {
    "type": "git",
    "url": "https://github.com/renovatebot/osv-offline.git"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "eslint": "eslint .",
    "prepare": "husky",
    "prettier": "prettier --cache -c -u \"**/*.*\"",
    "prettier-fix": "prettier --cache -w -u \"**/*.*\"",
    "start": "tsx ./packages/osv-offline-updater/src/index.ts",
    "test": "run-s eslint test:unit test:integration",
    "test:unit": "vitest run unit.spec.ts",
    "test:integration": "vitest run int.spec.ts --poolOptions.threads.singleThread"
  },
  "devDependencies": {
    "@containerbase/eslint-plugin": "1.1.15",
    "@containerbase/semantic-release-pnpm": "1.2.7",
    "@eslint/js": "9.37.0",
    "@tsconfig/node18": "18.2.4",
    "@tsconfig/strictest": "2.0.6",
    "@vitest/coverage-v8": "3.2.4",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-formatter-gha": "1.6.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-promise": "7.2.1",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.4",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "semantic-release": "24.2.9",
    "ts-node": "10.9.2",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=22.12.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.18.2"
}
